<IfModule mod_rewrite.c>
   RewriteEngine on
   RewriteCond %{HTTP_REFERER} !^http://(www\.)?localhost/7maru [NC] 
RewriteCond %{HTTP_REFERER} !^http://(www\.)?localhost/7maru.*$ [NC] 
RewriteRule \.(gif|jpg|swf)$ - [F]   
   RewriteRule    ^$ app/webroot/ [L]
   RewriteRule    (.*) app/webroot/$1 [L]
   
   </IfModule>

